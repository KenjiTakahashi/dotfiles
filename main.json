{
    "link": {
        "dotgitignore": "~/.gitignore",
        "dotvimrc": "~/.vimrc",
        "dotgtkrc-2.0": "~/.gtkrc-2.0",
        "dotinterrobangrc": "~/.interrobangrc",
        "dotxinitrc": "~/.xinitrc",
        "dotxprofile": "~/.xprofile",
        "dotXresources": "~/.Xresources",
        "dotzshrc": "~/.zshrc",
        "dotvim": "~/.vim",
        "dotconfig": [
            "~/.config", {
                "bspwm": "bspwm",
                "burp": "burp",
                "osop": "osop",
                "sxhkd": "sxhkd",
                "wingo": "wingo",
                "ranger": "ranger",
                "termite": "termite"
            }
        ]
    },
    "commands": [
        "vim +PlugUpdate +qall",
        "git clone https://github.com/robbyrussell/oh-my-zsh.git ~/.oh-my-zsh"
    ]
}
