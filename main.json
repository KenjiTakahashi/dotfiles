{
    "link": {
        "dotvimrc": "~/.vimrc",
        "dotgtkrc-2.0": "~/.gtkrc-2.0",
        "dotinterrobangrc": "~/.interrobangrc",
        "dotxinitrc": "~/.xinitrc",
        "dotxprofile": "~/.xprofile",
        "dotXresources": "~/.Xresources",
        "dotzshrc": "~/.zshrc",
        "dotvim": "~/.vim",
        "dotconfig": [
            "~/.config", {
                "bspwm": "bspwm",
                "burp": "burp",
                "osop": "osop",
                "sxhkd": "sxhkd",
                "wingo": "wingo",
                "ranger": "ranger",
                "termite": "termite"
            }
        ]
    },
    "commands": [
        "git clone https://github.com/gmarik/Vundle.vim.git ~/.vim/bundle/Vundle.vim",
        "vim +PluginUpdate +qall",
        "mkdir -p ~/.vim/ycm && cd ~/.vim/ycm && cmake -G 'Unix Makefiles' ../bundle/YouCompleteMe/third_party/ycmd/cpp && make ycm_support_libs",
        "git clone https://github.com/robbyrussell/oh-my-zsh.git ~/.oh-my-zsh"
    ]
}
