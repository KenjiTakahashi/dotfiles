{
    "vars": {
        "xrandr": "--output LVDS-0 --output HDMI-0 --right-of LVDS-0",
        "wallpaper": "$HOME/wallpapers/46A5B1599.jpg"
    },
    "links": {
        "dotgitignore": "~/.gitignore",
        "dotvimrc": "~/.vimrc",
        "dotgtkrc-2.0": "~/.gtkrc-2.0",
        "dotinterrobangrc": "~/.interrobangrc",
        "dotxinitrc": "~/.xinitrc",
        "dotxprofile": {
            "destination": "~/.xprofile",
            "templates": {
                "xrandr": "{xrandr}",
                "wallpaper": "{wallpaper} {wallpaper}"
            }
        },
        "dotXresources": "~/.Xresources",
        "dotzshrc": {
            "destination": "~/.zshrc",
            "templates": {
                "xrandr": "{xrandr}",
                "wallpaper": "{wallpaper} {wallpaper}",
                "theme": "afowler"
            }
        },
        "dotvim": "~/.vim",
        "dotdircolors": "~/.dircolors",
        "dotconfig": {
            "destination": "~/.config",
            "links": {
                "bspwm": "bspwm",
                "burp": "burp",
                "gtk-3.0": "gtk-3.0",
                "osop": "osop",
                "sxhkd": "sxhkd",
                "wingo": "wingo",
                "ranger": "ranger",
                "termite": "termite"
            }
        }
    },
    "commands": [
        "vim +PlugUpdate +qall",
        "git clone https://github.com/robbyrussell/oh-my-zsh.git ~/.oh-my-zsh"
    ]
}
